{
  "name": "@ldesign/tracker-vue",
  "version": "1.0.0",
  "description": "LDesign Vue 用户行为追踪组件 - v-track 指令、composables",
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs",
      "default": "./esm/index.js"
    },
    "./composables": {
      "types": "./esm/composables/index.d.ts",
      "import": "./esm/composables/index.js",
      "require": "./cjs/composables/index.cjs",
      "default": "./esm/composables/index.js"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./cjs/composables/*.cjs",
      "default": "./esm/composables/*.js"
    },
    "./directives": {
      "types": "./esm/directives/index.d.ts",
      "import": "./esm/directives/index.js",
      "require": "./cjs/directives/index.cjs",
      "default": "./esm/directives/index.js"
    },
    "./directives/*": {
      "types": "./esm/directives/*.d.ts",
      "import": "./esm/directives/*.js",
      "require": "./cjs/directives/*.cjs",
      "default": "./esm/directives/*.js"
    },
    "./plugin": {
      "types": "./esm/plugin/index.d.ts",
      "import": "./esm/plugin/index.js",
      "require": "./cjs/plugin/index.cjs",
      "default": "./esm/plugin/index.js"
    },
    "./plugin/*": {
      "types": "./esm/plugin/*.d.ts",
      "import": "./esm/plugin/*.js",
      "require": "./cjs/plugin/*.cjs",
      "default": "./esm/plugin/*.js"
    },
    "./plugins": {
      "types": "./esm/plugins/index.d.ts",
      "import": "./esm/plugins/index.js",
      "require": "./cjs/plugins/index.cjs",
      "default": "./esm/plugins/index.js"
    },
    "./plugins/*": {
      "types": "./esm/plugins/*.d.ts",
      "import": "./esm/plugins/*.js",
      "require": "./cjs/plugins/*.cjs",
      "default": "./esm/plugins/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "esm",
    "cjs",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs=require('fs'); ['es','esm','cjs','lib','dist','.tsbuildinfo'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch{}})\""
  },
  "dependencies": {
    "@ldesign/tracker-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "vue": "^3.5.13"
  },
  "typings": "./esm/index.d.ts",
  "browser": "./dist/index.js"
}