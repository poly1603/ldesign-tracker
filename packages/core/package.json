{
  "name": "@ldesign/tracker-core",
  "version": "1.0.0",
  "description": "LDesign 用户行为追踪核心库 - 框架无关的行为收集和分析",
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs",
      "default": "./es/index.js"
    },
    "./collectors": {
      "types": "./es/collectors/index.d.ts",
      "import": "./es/collectors/index.js",
      "require": "./lib/collectors/index.cjs",
      "default": "./es/collectors/index.js"
    },
    "./collectors/*": {
      "types": "./es/collectors/*.d.ts",
      "import": "./es/collectors/*.js",
      "require": "./lib/collectors/*.cjs",
      "default": "./es/collectors/*.js"
    },
    "./tracker": {
      "types": "./es/tracker/index.d.ts",
      "import": "./es/tracker/index.js",
      "require": "./lib/tracker/index.cjs",
      "default": "./es/tracker/index.js"
    },
    "./tracker/*": {
      "types": "./es/tracker/*.d.ts",
      "import": "./es/tracker/*.js",
      "require": "./lib/tracker/*.cjs",
      "default": "./es/tracker/*.js"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs",
      "default": "./es/types/index.js"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs",
      "default": "./es/types/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs=require('fs'); ['es','lib','dist','.tsbuildinfo'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch{}})\""
  },
  "dependencies": {
    "@ldesign/shared": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "typings": "./es/index.d.ts",
  "browser": "./dist/index.js"
}