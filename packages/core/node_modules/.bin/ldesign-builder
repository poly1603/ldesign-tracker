#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/workspace/ldesign/tools/builder/bin/node_modules:/mnt/d/workspace/ldesign/tools/builder/node_modules:/mnt/d/workspace/ldesign/tools/node_modules:/mnt/d/workspace/ldesign/node_modules:/mnt/d/workspace/node_modules:/mnt/d/node_modules:/mnt/d/workspace/ldesign/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/workspace/ldesign/tools/builder/bin/node_modules:/mnt/d/workspace/ldesign/tools/builder/node_modules:/mnt/d/workspace/ldesign/tools/node_modules:/mnt/d/workspace/ldesign/node_modules:/mnt/d/workspace/node_modules:/mnt/d/node_modules:/mnt/d/workspace/ldesign/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tools/builder/bin/ldesign-builder.js" "$@"
else
  exec node  "$basedir/../../../../../../tools/builder/bin/ldesign-builder.js" "$@"
fi
